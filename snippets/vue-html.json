  "ion-form": {
    "prefix": "i-form",
    "body": [
      "<form [formGroup]=\"${1:loginForm}\" (ngSubmit)=\"${2:onSubmit}()\">",
      "\t<ion-item>",
      "\t\t<ion-label position=\"${3|floating,inline,fixed,stacked|}\">${4:E-Mail}</ion-label>",
      "\t\t<ion-input formControlName=\"${5:email}\" type=\"${6|text,password,email,number,tel,url|}\"></ion-input>",
      "\t</ion-item>",
      "\t<ion-item>",
      "\t\t<ion-label position=\"${3|floating,inline,fixed,stacked|}\">${7:Password}</ion-label>",
      "\t\t<ion-input formControlName=\"${8:password}\" type=\"${9|text,password,email,number,tel,url|}\"></ion-input>",
      "\t</ion-item>",
      "\t<ion-button block color=\"${10|primary,secondary,tertiary|}\" type=\"submit\" [disabled]=\"${1:loginForm}.invalid\">",
      "\t\t${11:Login}",
      "\t</ion-button>",
      "</form>"
    ],
    "description": "<ion-form> w/ <ion-input> and formGroup"
  },
  "ion-form-error": {
    "prefix": "i-form-error",
    "body": [
      "<form [formGroup]=\"${1:loginForm}\" (ngSubmit)=\"${2:onSubmit}()\">",
