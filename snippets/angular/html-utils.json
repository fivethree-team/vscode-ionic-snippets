{
  "ion-align-items": {
    "prefix": "i-align-items",
    "body": [
      "class=\"ion-align-items-${1|start,end,center,baseline,stretch|}\""
    ],
    "description": "CSS Utilities Flex Container Align Items"
  },
  "ion-align-self": {
    "prefix": "i-align-self",
    "body": [
      "class=\"ion-align-self-${1|start,end,center,baseline,stretch,auto|}\""
    ],
    "description": "CSS Utilities Flex Container Align Self"
  },
  "ion-click": {
    "prefix": "i-click",
    "body": ["(click)=\"${1:onClick()}\""],
    "description": "(click) event"
  },
  "ion-color": {
    "prefix": "i-color",
    "body": [
      "color=\"${1|primary,secondary,tertiary,success,warning,danger,light,medium,dark|}\""
    ],
    "description": "(click) event"
  },
  "ion-col-offset": {
    "prefix": "i-col-offset",
    "body": ["offset=\"${2:2}\""],
    "description": "Ionic col [offset] property selection"
  },
  "ion-col-offset-responsive": {
    "prefix": "i-col-offset-responsive",
    "body": ["offset-${1|sm,md,lg,xl|}=\"${2:2}\""],
    "description": "Ionic col [offset] property selection w/ breakpoints"
  },
  "ion-col-pull": {
    "prefix": "i-col-pull",
    "body": ["pull=\"${2:2}\""],
    "description": "Ionic col pull property selection"
  },
  "ion-col-pull-responsive": {
    "prefix": "i-col-pull-responsive",
    "body": ["pull-${1|sm,md,lg,xl|}=\"${2:2}\""],
    "description": "Ionic col pull property selection w/ breakpoints"
  },
  "ion-col-push": {
    "prefix": "i-col-push",
    "body": ["push=\"${2:12}\""],
    "description": "Ionic col [push] property selection"
  },
  "ion-col-push-responsive": {
    "prefix": "i-col-push-responsive",
    "body": ["push-${1|sm,md,lg,xl|}=\"${2:12}\""],
    "description": "Ionic col [push] property selection w/ breakpoints"
  },
  "ion-col-size": {
    "prefix": "i-col-size",
    "body": ["size=\"${2:12}\""],
    "description": "Ionic col size property selection"
  },
  "ion-col-size-responsive": {
    "prefix": "i-col-size-responsive",
    "body": ["size-${1|sm,md,lg,xl|}=\"${2:12}\""],
    "description": "Ionic col size property selection w/ breakpoints"
  },
  "ion-flex-wrap": {
    "prefix": "i-flex-wrap",
    "body": ["class=\"ion-${1|nowrap,wrap,wrap-reverse|}\""],
    "description": "CSS Utilities Flex Container Flex Wrap"
  },
  "ion-float-element": {
    "prefix": "i-float-element",
    "body": ["class=\"ion-float-${1|left,right,end,start|}\""],
    "description": "CSS Utilities Float Elements"
  },
  "ion-float-element-responsive": {
    "prefix": "i-float-element-responsive",
    "body": [
      "class=\"ion-float-${1|sm,md,lg,xl|}-${2|left,right,end,start|}\""
    ],
    "description": "CSS Utilities Responsive Float Elements"
  },
  "ion-hide": {
    "prefix": "i-hide",
    "body": ["class=\"ion-hide\""],
    "description": "CSS Utilities Hide Element Class"
  },
  "ion-hide-breakpoints": {
    "prefix": "i-hide-breakpoints",
    "body": ["class=\"ion-hide-${1|sm,md,lg,xl|}-${2|up,down|}\""],
    "description": "CSS Utilities Hide Element with Display Breakpoints"
  },
  "ion-hide-sm": {
    "prefix": "i-hide-sm",
    "body": ["class=\"ion-hide-sm-${1|up,down|}\""],
    "description": "CSS Utilities Hide Element sm. Applies the modifier to the element when min-width: 576px (up) or max-width: 576px (down)."
  },
  "ion-hide-md": {
    "prefix": "i-hide-md",
    "body": ["class=\"ion-hide-md-${1|up,down|}\""],
    "description": "CSS Utilities Hide Element md. Applies the modifier to the element when min-width: 768px (up) or max-width: 768px (down)."
  },
  "ion-hide-lg": {
    "prefix": "i-hide-lg",
    "body": ["class=\"ion-hide-lg-${1|up,down|}\""],
    "description": "CSS Utilities Hide Element lg. Applies the modifier to the element when min-width: 992px (up) or max-width: 992px (down)."
  },
  "ion-hide-xl": {
    "prefix": "i-hide-xl",
    "body": ["class=\"ion-hide-xl-${1|up,down|}\""],
    "description": "CSS Utilities Hide Element xl. Applies the modifier to the element when min-width: 1200px (up) or max-width: 1200px (down)."
  },
  "ion-justify-container": {
    "prefix": "i-justity-content",
    "body": [
      "class=\"ion-justify-content-${1|start,end,center,around,between,evenly|}\""
    ],
    "description": "CSS Utilities Flex Container Justify Content"
  },
  "ion-margin": {
    "prefix": "i-margin",
    "body": ["class=\"ion-margin\""],
    "description": "CSS Utilities Element Margin"
  },
  "ion-margin-type": {
    "prefix": "i-margin-type",
    "body": [
      "class=\"ion-margin-${1|top,start,end,bottom,vertical,horizontal|}\""
    ],
    "description": "CSS Utilities Element Margin Type"
  },
  "ion-no-margin": {
    "prefix": "i-no-margin",
    "body": ["class=\"ion-no-margin\""],
    "description": "CSS Utilities Element No Margin"
  },
  "ion-no-border": {
    "prefix": "i-no-border",
    "body": ["class=\"ion-no-border\""],
    "description": "CSS Utilities Element No Border for <ion-header> or <ion-footer>"
  },
  "ion-no-padding": {
    "prefix": "i-no-padding",
    "body": ["class=\"ion-no-padding\""],
    "description": "CSS Utilities Element No Padding"
  },
  "ion-padding": {
    "prefix": "i-padding",
    "body": ["class=\"ion-padding\""],
    "description": "CSS Utilities Element Padding"
  },
  "ion-padding-type": {
    "prefix": "i-padding-type",
    "body": [
      "class=\"ion-padding-${1|top,start,end,bottom,vertical,horizontal|}\""
    ],
    "description": "CSS Utilities Element Padding Type"
  },
  "scrollEvents": {
    "prefix": "i-scrollEvents",
    "body": ["[scrollEvents]=\"${1|true,false|}\""],
    "description": "Ionic [scrollEvents] binding for <ion-content>"
  },
  "slot": {
    "prefix": "i-slot",
    "body": ["slot=\"${1|start,end,primary,secondary|}\""],
    "description": "Ionic slot property"
  },
  "ion-text-alignment": {
    "prefix": "i-text-alignment",
    "body": [
      "class=\"ion-text-${1|left,right,start,end,center,justify,wrap,nowrap|}\""
    ],
    "description": "CSS Utilities Text Alignment"
  },
  "ion-text-responsive": {
    "prefix": "i-text-responsive",
    "body": [
      "class=\"ion-text-${1|sm,md,lg,xl|}-${2|left,right,start,end,center,justify,wrap,nowrap,uppercase,lowercase,capitalize|}\""
    ],
    "description": "CSS Utilities Responsive Text Classes for alignment and transformation"
  },
  "ion-text-transformation": {
    "prefix": "i-text-transformation",
    "body": ["class=\"ion-text-${1|uppercase,lowercase,capitalize|}\""],
    "description": "CSS Utilities Text Transformation"
  }
}
