{
    "name": "vscode-ionic-snippets",
    "displayName": "Ionic 4 Snippets",
    "description": "Ionic 4 snippets for VS Code",
    "version": "0.14.0",
    "author": "Marc Stammerjohann",
    "license": "MIT",
    "publisher": "fivethree",
    "galleryBanner": {
        "color": "#3880ff",
        "theme": "dark"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/fivethree-team/vscode-ionic-snippets.git"
    },
    "bugs": {
        "url": "https://github.com/fivethree-team/vscode-ionic-snippets/issues"
    },
    "engines": {
        "vscode": "^1.26.0"
    },
    "categories": [
        "Snippets"
    ],
    "keywords": [
        "Ionic",
        "Ionic v4",
        "TypeScript",
        "HTML",
        "Snippets"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "html",
                "path": "./snippets/html.json"
            },
            {
                "language": "scss",
                "path": "./snippets/scss.json"
            },
            {
                "language": "typescript",
                "path": "./snippets/typescript.json"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test",
        "vscode:publish": "vsce publish",
        "version": "conventional-changelog -p angular -i ./CHANGELOG.md -s"
    },
    "devDependencies": {
        "@types/mocha": "5.2.5",
        "@types/node": "10.12.18",
        "tslint": "5.12.1",
        "typescript": "2.9.2",
        "vscode": "1.1.28"
    }
}
