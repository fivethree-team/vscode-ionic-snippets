{
  "name": "vscode-ionic-snippets",
  "displayName": "Ionic Snippets",
  "description": "Ionic snippets for VS Code",
  "version": "2.2.0",
  "author": "Marc Stammerjohann",
  "license": "MIT",
  "publisher": "fivethree",
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/fivethree-team/vscode-ionic-snippets.git"
  },
  "bugs": {
    "url": "https://github.com/fivethree-team/vscode-ionic-snippets/issues"
  },
  "engines": {
    "vscode": "^1.26.0"
  },
  "categories": [
    "Snippets"
  ],
  "keywords": [
    "Ionic",
    "Ionic v4",
    "Ionic v5",
    "Stencil",
    "React",
    "Vue",
    "TypeScript",
    "HTML",
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "scss",
        "path": "./snippets/scss.json"
      },
      {
        "language": "html",
        "path": "./snippets/angular/html.json"
      },
      {
        "language": "html",
        "path": "./snippets/angular/html-utils.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/angular/typescript.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/stencil/html-tsx.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/react/html-tsx.json"
      },
      {
        "language": "vue-html",
        "path": "./snippets/vue/html.json"
      }
    ]
  },
  "scripts": {
    "vscode:publish": "vsce publish",
    "version": "conventional-changelog -p angular -i ./CHANGELOG.md -s"
  }
}
